<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="vm"
            type="com.ds.worldfunclub.viewmodel.CategoryLastFragmentModel" />
    </data>

    <androidx.recyclerview.widget.RecyclerView android:background="@color/colorBackgroundMain"
        adapter="@{vm.adapter}"
        android:layout_width="match_parent" android:paddingTop="8dp" android:paddingStart="10.5dp" android:paddingEnd="10.5dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
        app:spanCount="2">

    </androidx.recyclerview.widget.RecyclerView>
</layout>